<form method="post" action="/user/update">
<table border=1>
<tr>
<th>Username
</th>
<th>Password
</th>
</tr>
<% user.forEach(function(user) { %>
<tr>
<td>
<input type="text" name="usernm" value=<%= user.username %>>
<input type="hidden" name="userid" value=<%= user.id %>>
</td>
<td><input type="text" name="passwd" value=<%= user.password %>></td>
<td><a href="/user/delete/<%= user.id %>"><input type="button" value="Delete"></a>
<td><input type="Submit" value="Update"></td>
</tr>
<% }); %>
</table>
</form>
